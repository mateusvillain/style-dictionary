{
  "colors": {
    "$type": "color",
    "dark": {
      "black": {
        "$value": "#ffffff"
      },
      "white": {
        "$value": "#1a1a1a"
      },
      "orange": {
        "100": {
          "$value": "#7b341e"
        },
        "200": {
          "$value": "#9c4221"
        },
        "300": {
          "$value": "#c05621"
        },
        "400": {
          "$value": "#dd6b20"
        },
        "500": {
          "$value": "#ed8936"
        },
        "600": {
          "$value": "#f6ad55"
        },
        "700": {
          "$value": "#fbd38d"
        },
        "800": {
          "$value": "#feebc8"
        },
        "900": {
          "$value": "#fffaf0"
        }
      }
    }
  },
  "borders": {
    "$type": "border",
    "dark": {
      "heavy": {
        "$value": {
          "color": "{colors.dark.white}",
          "width": "{dimensions.1}",
          "style": "{borders.styles.solid}"
        }
      },
      "wireframe": {
        "$value": {
          "color": "{colors.dark.orange.400}",
          "width": "{dimensions.2}",
          "style": "{borders.styles.dashed}"
        }
      }
    }
  },
  "shadows": {
    "$type": "shadow",
    "dark": {
      "sm": {
        "$value": {
          "color": "{colors.dark.white}",
          "offsetX": "{dimensions.0}",
          "offsetY": "{dimensions.1}",
          "blur": "{dimensions.3}"
        }
      },
      "lg": {
        "$value": {
          "color": "{colors.dark.white}",
          "offsetX": "{dimensions.0}",
          "offsetY": "{dimensions.2}",
          "blur": "{dimensions.4}"
        }
      },
      "multi": {
        "$value": ["{shadows.dark.sm}", "{shadows.dark.lg}"]
      }
    }
  }
}

